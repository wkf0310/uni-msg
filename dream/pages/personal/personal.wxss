/* pages/personal/personal.wxss */
.nav{
    width: 100%;
    height: 100rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.nav image{
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    left: 14rpx;
}
.nav text{
    font-weight: 900;
}
.ul .li{
    box-sizing: border-box;
    padding: 20rpx 20rpx;
    display: flex;
    justify-content: space-between;
    border-bottom: 1px solid #eee;
}
.ul .li image{
    width: 30rpx;
    height: 30rpx;
}