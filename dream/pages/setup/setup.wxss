/* pages/setup/setup.wxss */
.nav{
    width: 100%;
    height: 100rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.nav image{
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    left: 14rpx;
}
.nav text{
    font-weight: 900;
}
.ul .li{
    box-sizing: border-box;
    padding: 20rpx 20rpx;
    display: flex;
    justify-content: space-between;
    border-bottom: 1px solid #eee;
}
.ul .li image{
    width: 30rpx;
    height: 30rpx;
}
.ul .li:nth-child(4){
    border-bottom: 0;
}
.foot{
   
    margin-top: 200rpx;
}
.foot button{
    background-color: #fff;
    color: #e96131;
    font-weight: 0;
    width: 660rpx !important;
    border: 1px solid #e96131;
}