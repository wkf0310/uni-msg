/* pages/study/study.wxss */

.hone-content .lb-item .lesson-not-sell {
    position: absolute;
    right: 10rpx;
    top: 20rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    width: 80rpx;
    height: 50rpx;
}

.hone-content .lb-item .lbi-teacher view text {
    margin-left: 10rpx;
}

.hone-content .lb-item .lbi-teacher view {
    display: flex;
    align-items: center;
}

.hone-content .lb-item .lbi-teacher {
    display: flex;
    color: #666666;
    font-size: 25rpx;
}

.hone-content .lb-item .label-type {
    font-size: 25rpx;
    margin: 20rpx 0;
}

.hone-content .lb-item .label-type text {
    background-color: #f4f4f4;
    width: 100rpx;
    height: 40rpx;
    display: inline-block;
    line-height: 40rpx;
    margin-right: 10rpx;
    text-align: center;
    color: #999999;
    border-radius: 5rpx;
}

.hone-content .lb-item .lb-title {
    margin-top: 30rpx;
    font-size: 28rpx;
}

.hone-content .lb-item .lb-label {
    position: absolute;
    left: 0;
    top: 0;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    width: 100rpx;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 23rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #fff;
    text-align: center;
    overflow: hidden;
}

.hone-content .lb-item {
    background: #fff;
    box-shadow: 0 2px 2px 1px hsl(0deg 0% 81% / 35%);
    border-radius: 10rpx;
    padding: 20rpx;
    position: relative;
    margin-top: 50rpx;
}

.hone-content .lb-item image {
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
}

.hone-content {
    padding: 10rpx;
}

.hone .hone-title {
    color: #424963;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.hone .hone-title view:nth-child(1) {
    font-size: 35rpx;
    font-weight: 600;
}

.hone .hone-title view:nth-child(2) {
    font-size: 25rpx;
}

.hone {
    padding: 10rpx;
}

.learn {
    display: flex;
    justify-content: space-around;
    margin: 30rpx 0;
}

.learn view {
    text-align: center;
    font-size: 25rpx;
}

.learn view image {
    width: 90rpx;
    height: 80rpx;
}

.weekcourse .head .right {
    font-size: 25rpx;
    color: #b3a7c6;
}

.weekcourse .head {
    width: 90%;
    margin: 10rpx auto;
    background-color: #f5f6f8;
    display: flex;
    align-items: center;
    justify-content: space-between;
    text-align: center;
    padding: 10rpx 30rpx;
}

.weekcourse .head .left {
    font-size: 35rpx;
}

.weekcourse .head .left text:nth-child(1) {
    color: #f15a24;
}

.weekcourse .head .left text:nth-child(2) {
    font-size: 28rpx;
}

.weekcourse .head image {
    width: 40rpx;
    height: 40rpx;
}

.weekcourse .week {
    display: flex;
    justify-content: space-between;
    font-size: 30rpx;
    color: #424963;
    text-align: center;
}

.weekcourse .week .active {
    width: 55rpx;
    height: 50rpx;
    background-color: #f15a24;
    border-radius: 50%;
    color: #fff;
    line-height: 50rpx;
    box-shadow: 0 0 0.4rem 0 rgb(241 89 35 / 40%);
}

.weekcourse .week view view {
    margin: 5rpx 0;
    height: 50rpx;
    line-height: 50rpx;
}

.date {
    color: #424963;
    font-size: 50rpx;
    font-weight: 600;
}

.content {
    padding: 20rpx;
    background-color: #fafafa;
}